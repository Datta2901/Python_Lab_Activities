    "import socket\n",
    "\n",
    "s = socket.socket(socket.AF_INET,socket.SOCK_STREAM)\n",
    "host = socket.gethostname()\n",
    "port = 1024\n",
    "s.connect((host,port))\n",
    "msg = s.recv(100)\n",
    "print(msg.decode(\"utf-8\"))"
